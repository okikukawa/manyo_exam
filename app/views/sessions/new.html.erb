<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <div class="">
    <%= f.label :email %>
    <br>
    <%= f.email_field :email %>
  </div>
  <div class="">
    <%= f.label :password %>
    <br>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "Log in" %>
<% end %>
